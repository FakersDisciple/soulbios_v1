{
  "version": 2,
  "builds": [
    {
      "src": "deepconf/soulbios_api.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "deepconf/soulbios_api.py"
    }
  ],
  "env": {
    "GEMINI_API_KEY": "@gemini_api_key",
    "CHROMA_DB_PATH": "./chroma_db",
    "REDIS_URL": "@redis_url",
    "SOULBIOS_API_KEY": "@soulbios_api_key"
  },
  "functions": {
    "deepconf/soulbios_api.py": {
      "maxDuration": 30
    }
  }
}